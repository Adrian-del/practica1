 public static void main(String[] args) {
         int[][] aiMatriz = new int[10][10];
         for (int i = 0; i < 10; i++) {
             for (int j = 0; j < 10; j++) {
                 if(i==j){
                     aiMatriz[i][i]=1;
                 }
                 else{
                     aiMatriz[i][i]=0;
                 }
                 System.out.print("[" + aiMatriz[i][i] + "]");
             }
             System.out.print("\n");
        }
         
    }
